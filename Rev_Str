class Solution 
{
    public void reverseString(char[] s) 
    {
        int first = 0;
        int last = s.length - 1;
        char c;        
        while(first<last)
        {            
            c = s[last];
            s[last] = s[first];
            s[first] = c;
            first++;
            last--;            
        }            
    }
}
